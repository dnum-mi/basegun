    "pinia": "^2.1.7",
    "stylelint-config-recommended-vue": "^1.5.0",
    "swiper": "^11.0.6",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@testing-library/cypress": "^10.0.1",
    "@types/jsdom": "^21.1.6",
